/* light/success/success.wxss */
page{
  overflow: hidden;
  background: rgb(255, 255, 255);
}
.scroll-box{
  position: absolute;
  top: -10%;
  left: 0;
  bottom: 0;
  height: 100%;
  width: 100%;
  white-space: nowrap;
}
.card-box{
  display: inline-block;
}
.card{
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  height: 95vh;
  width: 100vw;
  margin: 0vh 0vw;
  font-size: 40px;
  border-radius: 0px;
}
.image1{
  top: 0rem;
  left: 0rem;
  height: 100%;
  width: 100%;
  align-items: center;
  justify-content: center;
}
.zhuye{
  position:fixed;
  bottom: 3%;
  left: 2%;
  width: 18%;
  height: 10%;
  z-index: 5;
}
.next{
  position: fixed;
  bottom: 2%;
  right: 2%;
  width: 18%;
  height: 10%;
  z-index: 5;
}